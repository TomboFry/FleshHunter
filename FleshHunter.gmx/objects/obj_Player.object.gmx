            <string>/// Movement
moving = false;
if(keyboard_check(vk_shift)){
    global.mspeed = 1.85;
} else {
    global.mspeed = 1.25;
}
if(keyboard_check(ord('A')) &amp;&amp; place_free(x-global.mspeed,y)){
    x -= global.mspeed;
    moving = true;
    movedir = -1;
}
if(keyboard_check(ord('D')) &amp;&amp; place_free(x+global.mspeed,y)){
    x += global.mspeed;
    moving = true;
    movedir = 1;
}
if(place_free(x,y+1)){
    gravity = 0.25;
    gravity_direction = 270;
    sprite_index = spr_PlayerJump;
    if(vspeed &gt; 0){
        image_index = 1;
    } else {
        image_index = 0;
    }
} else {
    gravity = 0;
    if(moving == true){
        sprite_index = spr_PlayerWalk;
        image_speed = (17/60) * (global.facing*movedir);
    } else {
        sprite_index = spr_Player;
        image_speed = 0;
    }
}
if(keyboard_check_pressed(ord('W')) &amp;&amp; place_meeting(x,y+1,obj_CollisionParent)){
    vspeed = -global.jheight;
}
if(keyboard_check_released(ord('W'))){
    if(vspeed &lt; 0){
        vspeed = vspeed / 2;
    }
}
